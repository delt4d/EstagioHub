{
    "name": "back-end",
    "version": "1.0.0",
    "main": "index.js",
    "scripts": {
        "dev": "ts-node-dev src/main.ts",
        "format": "prettier --write src/*.ts && eslint --fix .",
        "prepare": "cd .. && husky back-end/.husky",
        "test": "jest --runInBand",
        "test:ci": "jest --ci --coverage",
        "test:watch": "jest --watch --runInBand",
        "build": "tsc --project tsconfig.prod.json",
        "setup:production": "npm pkg delete scripts.prepare && npm ci --only=production",
        "start": "node dist/main.js",
        "start:production": "npm run migrate && npm start",
        "migrate": "npm run migration:migrate",
        "postmigrate": "npm run migration:seed:all",
        "migration:migrate": "NODE_ENV=production sequelize-cli db:migrate",
        "migration:create": "sequelize-cli migration:create --name",
        "migration:seed:generate": "sequelize-cli seed:generate --name",
        "migration:seed:all": "sequelize-cli db:seed:all"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "description": "",
    "devDependencies": {
        "@swc/core": "^1.7.26",
        "@swc/jest": "^0.2.36",
        "@types/bcrypt": "^5.0.2",
        "@types/cookie-parser": "^1.4.7",
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/express-rate-limit": "^6.0.0",
        "@types/express-session": "^1.18.0",
        "@types/ioredis": "^4.28.10",
        "@types/jest": "^29.5.12",
        "@types/joi": "^17.2.3",
        "@types/node": "^20.14.14",
        "@types/nodemailer": "^6.4.15",
        "@types/passport": "^1.0.16",
        "@types/passport-http-bearer": "^1.0.41",
        "@types/pg": "^8.11.10",
        "@types/sqlite3": "^3.1.11",
        "@types/supertest": "^6.0.2",
        "@types/validator": "^13.12.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.2.1",
        "husky": "^9.1.1",
        "jest": "^29.7.0",
        "lint-staged": "^15.2.7",
        "prettier": "^3.3.3",
        "prettier-plugin-organize-imports": "^4.0.0",
        "supertest": "^7.0.0",
        "ts-jest": "^29.2.4",
        "ts-node": "^10.9.2",
        "ts-node-dev": "^2.0.0",
        "typescript-eslint": "^7.16.1"
    },
    "dependencies": {
        "bcrypt": "^5.1.1",
        "connect-redis": "^7.1.1",
        "cookie-parser": "^1.4.6",
        "cors": "^2.8.5",
        "express": "^4.21.0",
        "express-async-errors": "^3.1.1",
        "express-rate-limit": "^7.4.0",
        "express-session": "^1.18.0",
        "express-slow-down": "^2.0.3",
        "helmet": "^7.1.0",
        "ioredis": "^5.4.1",
        "joi": "^17.13.3",
        "nodemailer": "^6.9.15",
        "passport": "^0.7.0",
        "passport-custom": "^1.1.1",
        "passport-http-bearer": "^1.0.1",
        "pg": "^8.13.1",
        "pg-hstore": "^2.3.4",
        "reflect-metadata": "^0.2.2",
        "sequelize": "^6.37.5",
        "sequelize-cli": "^6.6.2",
        "sequelize-typescript": "^2.1.6",
        "sqlite3": "^5.1.7",
        "typescript": "^5.6.2"
    },
    "lint-staged": {
        "*.{js,ts}": [
            "prettier --write",
            "eslint ./ --fix",
            "git add -A"
        ]
    }
}
